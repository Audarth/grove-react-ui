{
  "name": "muir-react-reference",
  "version": "0.1.0",
  "description": "Turning MarkLogic forests into a place for users to live",
  "dependencies": {},
  "devDependencies": {
    "concurrently": "^3.5.0"
  },
  "scripts": {
    "postinstall": "cd client && npm install && cd ../server && npm install --no-optional && cd ..",
    "start": "concurrently \"npm run server\" \"npm run client\" ",
    "server": "node server/node-app.js",
    "client": "node start-client.js",
    "test": "cd client && npm test",
    "mlDeploy": "cd marklogic && ./gradlew mlDeploy",
    "loadSampleData": "cd marklogic && ./gradlew loadSampleData",
    "setupEverything": "npm install && npm run mlDeploy && npm run loadSampleData && npm start"
  },
  "repository": {
    "type": "git",
    "url": "ssh://project.marklogic.com:7999/nacw/muir.git"
  },
  "contributors": [
    {
      "name": "Patrick McElwee",
      "email": "patrick.mcelwee@marklogic.com"
    },
    {
      "name": "Frank Sanders",
      "email": "frank.sanders@marklogic.com"
    }
  ]
}
